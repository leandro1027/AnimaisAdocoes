datasource db {
  provider = "sqlite"
  url      = "file:./dev.db" 
}

generator client {
  provider = "prisma-client-js"
}

// Example model

model Animal{
  id Int @id  @default(autoincrement())
  raca String

}

model Usuario{
  id Int @id  @default(autoincrement())
  nome String
  cpf String
  email String
}
